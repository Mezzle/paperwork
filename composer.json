{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"jenssegers/agent": "*",
		"erusev/parsedown": "dev-master",
		"thiagoalessio/tesseract_ocr": ">= 0.2.0",
		"simplesoftwareio/simple-qrcode": "1.1.*",
		"barryvdh/laravel-ide-helper": "~1.11",
		"webpatser/laravel-uuid": "1.*",
		"strebl/adldap": "^4.0",
		"php": "^5.6.0",
		"doctrine/dbal": "~2.5",
		"ext/mcrypt": "*"
	},
	"autoload": {
		"classmap": [
			"frontend/app/commands",
			"frontend/app/controllers",
			"frontend/app/models",
			"frontend/app/extensions",
			"frontend/app/lib",
			"frontend/app/database/migrations",
			"frontend/app/database/seeds",
			"frontend/app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-update-cmd": [
			"php artisan clear-compiled",
            "php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable",
	"require-dev": {
		"doctrine/dbal": "^2.5"
	}
}